    <!-- http://purl.jp/bio/10/mccv/000003 -->


    <owl:Class rdf:about="http://purl.jp/bio/10/mccv/000003">
        <rdfs:subClassOf rdf:resource="https://identifiers.org/data_1872"/>
        <dc:description xml:lang="en">Represents a group of microorganisms capable of interbreeding and producing fertile offspring. This designation distinguishes closely related microbial entities within the microbiota, aiding in the detailed characterization of microbial diversity.</dc:description>
        <rdfs:label>Species</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sig/ont/fma/265130 -->


    <owl:Class rdf:about="http://purl.org/sig/ont/fma/265130">
