    <!-- http://www.owl-ontologies.com/MESH/D002511 -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/MESH/D002511">
        <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/LNC/LP31426-7"/>
        <dc:description xml:lang="en">A class of beta-lactam antibiotics similar to penicillins but with a broader spectrum of activity. Cephalosporins are divided into several generations, each with varying effectiveness against Gram-positive and Gram-negative bacteria.</dc:description>
        <rdfs:comment xml:lang="en">The impact of cephalosporins on the microbiota varies by generation. While generally considered moderate in their effect, prolonged use can lead to alterations in the gut and respiratory tract microbiota.</rdfs:comment>
        <rdfs:label xml:lang="en">Cephalosporins</rdfs:label>
    </owl:Class>
    


    <!-- https://identifiers.org/data_1872 -->


    <owl:Class rdf:about="https://identifiers.org/data_1872"/>
    


    <!-- https://w3id.org/micronto/0001 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0001">
        <rdfs:subClassOf rdf:resource="http://vocabs.lter-europe.net/EnvThes/21962"/>
        <dc:description xml:lang="en">Live microorganisms, usually bacteria or yeast, that confer health benefits to the host when consumed in adequate amounts, contributing to the balance of the microbiota.</dc:description>
        <rdfs:label xml:lang="en">Probiotic</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0002 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0002">
        <dc:description xml:lang="en">Specific composition and diversity of microorganisms within the microbiota, considering variations in different body locations and environmental conditions.</dc:description>
        <rdfs:label>Microorganism Composition</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0003 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0003">
        <dc:description xml:lang="en">Positive contributions of the microbiota to human health, emphasizing the benefits of maintaining a balanced and diverse microbial community.</dc:description>
        <rdfs:label>Microbiota Benefits</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0004 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0004">
        <rdfs:subClassOf rdf:resource="https://identifiers.org/data_1872"/>
        <dc:description xml:lang="en">Represents the highest taxonomic rank in the classification of microorganisms, emphasizing the broad categorization of life forms within the microbiota.</dc:description>
        <rdfs:label>Domain</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0005 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0005">
        <dc:description xml:lang="en">How spatial variations within the body, such as different organ systems and tissue types, influence the composition and function of the microbiota.</dc:description>
        <rdfs:comment xml:lang="en">These changes can have substantial implications in comparative studies.</rdfs:comment>
        <rdfs:label>Spatial Changes</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0006 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0006">
        <dc:description xml:lang="en">Explores how changes in the external environment impact the microbiota, considering factors such as climate, pollution, and other external influences.</dc:description>
        <rdfs:comment xml:lang="en">Include both natural and anthropogenic changes here.</rdfs:comment>
        <rdfs:label>Environmental Changes</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0007 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0007">
        <dc:description xml:lang="en">Overall physiological systems of the human body, exploring the interactions and influences of the microbiota on these systems.</dc:description>
        <rdfs:label xml:lang="en">Human Physiological System</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/micronto/0008 -->


    <owl:Class rdf:about="https://w3id.org/micronto/0008">
        <dc:description xml:lang="en">Physiological pathways within the human body, considering how these pathways interact with and are influenced by the microbiota.</dc:description>
        <rdfs:label>Human Pathways</rdfs:label>
    </owl:Class>
    


